FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField uniform 0.03;
boundaryField
{
    INLET {
		type fixedValue;
		value uniform 0.03;
	}
	
	OUTLET {
		type zeroGradient;
	}
    
    wall {
		type omegaWallFunction;
		value uniform 0.03;
	}

        "(up|down)" {
	type symmetry;
	value uniform 0;
	}
}
